{
  "name": "@hadeshe93/vice-cli",
  "version": "0.3.0",
  "description": "A convenient scaffold named vice",
  "author": "hadeshe93 <hadeshe93@gmail.com>",
  "homepage": "https://github.com/hadeshe93/hh-lib#readme",
  "license": "ISC",
  "bin": {
    "vice": "bin/vice.cjs.js"
  },
  "module": "dist/index.node.esm.js",
  "main": "index.js",
  "exports": {
    "import": "./dist/index.node.esm.js",
    "require": "./dist/index.node.cjs.js"
  },
  "types": "dist/index.d.ts",
  "buildOptions": [
    {
      "name": "vice",
      "target": "node",
      "formats": [
        "cjsBin",
        "esmBin"
      ]
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "dist",
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hadeshe93/hh-lib.git"
  },
  "scripts": {
    "showconfig": "ts-node -P ./tsconfig.json --showConfig",
    "dev": "cross-env DEBUG=vice* ts-node -P ./tsconfig.json ./src/index.ts",
    "devx": "cross-env NODE_ENV=development TS_NODE_PROJECT=./tsconfig.json node -r ts-node/register src/index.ts",
    "build:dev": "cross-env NODE_ENV=development esno ../../scripts/build.ts vice-cli",
    "build": "cross-env NODE_ENV=production esno ../../scripts/build.ts vice-cli"
  },
  "bugs": {
    "url": "https://github.com/hadeshe93/hh-lib/issues"
  },
  "dependencies": {
    "@hadeshe93/lib-common": "workspace:^0.6.0",
    "@hadeshe93/lib-node": "workspace:^0.5.0",
    "@hadeshe93/webpack-config": "workspace:^0.2.0",
    "@types/inquirer": "^8.2.1",
    "chalk": "^4.1.1",
    "commander": "^9.3.0",
    "debug": "^4.3.4",
    "download-git-repo": "^3.0.2",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "inquirer": "^8.2.1",
    "ora": "^5.4.1",
    "tapable": "^2.2.1",
    "webpack": "^5.73.0",
    "webpack-dev-server": "^4.9.3"
  }
}
